{
  "mcpServers": {
    "browser-tools": {
      "command": "cmd.exe",
      "args": [
        "/c",
        "npx -y @agentdeskai/browser-tools-mcp@1.2.0"
      ],
      "enabled": true
    },
    "github": {
      "command": "cmd.exe",
      "args": [
        "/c",
        "npx -y @modelcontextprotocol/server-github@latest"
      ],
      "enabled": true,
      "env": {
        "GITHUB_PERSONAL_ACCESS_TOKEN": ""
      }
    }
  }
} 